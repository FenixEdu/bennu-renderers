    </attribute>

    <attribute>
      <name>type</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.String</type>
      <description>
        Selects the type of the messages to show. The type can be one of 'global', 'validation',
        or 'conversion'.
      </description>
    </attribute>
  </tag>
  
